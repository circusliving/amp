  "author": "Randy Houlahan <randy@houlahan.ca>",
  "private": true,
  "dependencies": {
    "@nuxtjs/apollo": "4.0.0-rc6",
    "@nuxtjs/axios": "5.5.4",
    "@nuxtjs/proxy": "^1.2.4",
